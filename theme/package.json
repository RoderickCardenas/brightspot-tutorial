{
  "name": "brightspot-tutorial-theme",
  "version": "1.0.0-SNAPSHOT",
  "private": true,
  "scripts": {
    "lint:js": "eslint \"**/*.js\" \"**/*.ts\" --ignore-pattern \"build/**/*\" --no-error-on-unmatched-pattern",
    "lint:misc": "prettier \"**/*.hbs\" \"**/*.json\" --check",
    "lint": "yarn lint:js && yarn lint:misc",
    "format:js": "yarn lint:js --fix",
    "format:misc": "yarn lint:misc --write",
    "format": "yarn format:js && yarn format:misc",
    "build": "yarn format && webpack --config webpack.prod.ts && node node_modules/@brightspot/styleguide/build/bundle",
    "styleguide": "webpack serve --config webpack.dev.ts"
  },
  "dependencies": {
    "core-js": "3.15.2"
  },
  "devDependencies": {
    "@babel/core": "7.14.8",
    "@babel/eslint-parser": "7.14.7",
    "@babel/preset-env": "7.14.8",
    "@babel/preset-typescript": "7.14.5",
    "@babel/register": "7.14.5",
    "@brightspot/styleguide": "4.2.21-rc.6",
    "@types/copy-webpack-plugin": "8.0.1",
    "@types/mini-css-extract-plugin": "2.0.1",
    "@typescript-eslint/eslint-plugin": "4.28.5",
    "@typescript-eslint/parser": "4.28.5",
    "babel-loader": "8.2.2",
    "copy-webpack-plugin": "9.0.1",
    "css-loader": "6.2.0",
    "eslint": "7.31.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-webpack-plugin": "3.0.1",
    "file-loader": "6.2.0",
    "less": "4.1.1",
    "less-loader": "10.0.1",
    "mini-css-extract-plugin": "2.1.0",
    "prettier": "2.3",
    "style-loader": "3.2.1",
    "typescript": "4.3.5",
    "url-loader": "4.1.1",
    "webpack": "5.47.0",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "5.8.0"
  }
}
